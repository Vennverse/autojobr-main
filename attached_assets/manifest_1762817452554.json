{
   "action": {
      "chrome_style": false,
      "default_icon": {
         "128": "assets/icons/favicon-128.png",
         "16": "assets/icons/favicon-16.png",
         "32": "assets/icons/favicon-32.png",
         "48": "assets/icons/favicon-48.png"
      },
      "default_title": "Simplify Copilot"
   },
   "author": "Simplify Jobs Inc.",
   "background": {
      "service_worker": "js/background.bundle.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/contentScript.bundle.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": {
      "extension_pages": "default-src 'self' 'wasm-unsafe-eval'; style-src 'self' 'wasm-unsafe-eval' 'sha256-p9ZiOd7f9dJhTZCQXDnv1mMXzX1CSCY3DevRUA9ivXM='; connect-src * data:; img-src 'self' https://storage.googleapis.com https://res.cloudinary.com https://ui-avatars.com data:"
   },
   "description": "The better way to job search. Quick apply and autofill job applications in one click. Track jobs, companies, resumes & more!",
   "externally_connectable": {
      "matches": [ "https://*.simplify.jobs/*", "https://*.village.do/*" ]
   },
   "homepage_url": "https://simplify.jobs/",
   "host_permissions": [ "*://*/*" ],
   "icons": {
      "128": "assets/icons/favicon-128.png",
      "16": "assets/icons/favicon-16.png",
      "32": "assets/icons/favicon-32.png",
      "48": "assets/icons/favicon-48.png"
   },
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "109",
   "name": "Simplify Copilot - Autofill job applications, job tracker & AI resumes",
   "permissions": [ "activeTab", "alarms", "cookies", "contextMenus", "offscreen", "storage", "tabs", "unlimitedStorage", "webNavigation", "webRequest" ],
   "short_name": "Simplify Copilot",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.2.9",
   "web_accessible_resources": [ {
      "matches": [ "*://*/*" ],
      "resources": [ "assets/*", "css/*", "js/*" ]
   } ]
}
